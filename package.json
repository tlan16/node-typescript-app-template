{
  "name": "node-typescript-app-template",
  "version": "0.0.1",
  "main": "src/main.mts",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=development nodemon",
    "lint": "yarn lint:app && yarn lint:yaml",
    "lint:app": "eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "lint:yaml": "yarn dcr lint-yaml",
    "build": "rimraf dist && tsc --declaration",
    "dc": "docker-compose -f docker/docker-compose.yml",
    "dcr": "yarn dc run --rm",
    "test": "jest",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/assert": "^1.5.6",
    "@types/jest": "^29.1.2",
    "@types/node": "18.11.5",
    "@types/prettier": "2.7.1",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "5.41.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.1",
    "jest": "^29.1.2",
    "nodemon": "2.0.20",
    "prettier": "^2.6.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "packageManager": "yarn@3.2.4"
}
