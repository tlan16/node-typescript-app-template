{
  "name": "learn-node-esm",
  "type": "module",
  "scripts": {
    "start": "node --no-warnings=ExperimentalWarning --loader @swc-node/register/esm src/main.ts",
    "dev": "node --no-warnings=ExperimentalWarning --loader @swc-node/register/esm --watch src/main.ts",
    "build": "del dist && tsc --project tsconfig.build.json",
    "test": "node --no-warnings=ExperimentalWarning --loader @swc-node/register/esm --test **/*.spec.ts",
    "lint": "eslint --ext .ts src/"
  },
  "main": "src/main.ts",
  "engines": {
    "node": "^20.9.0",
    "yarn": "4.0.2"
  },
  "packageManager": "yarn@4.0.2",
  "devDependencies": {
    "@swc-node/register": "^1.6.8",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.96",
    "@total-typescript/ts-reset": "^0.5.1",
    "@tsconfig/node20": "^20.1.2",
    "@tsconfig/strictest": "^2.0.2",
    "@types/eslint": "^8.44.7",
    "@types/node": "^20.9.0",
    "@types/sinon": "^17",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "@typescript-eslint/parser": "^6.11.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.53.0",
    "sinon": "^17.0.1",
    "swc_mut_cjs_exports": "^0.86.17",
    "ts-node": "npm:@tlan16/ts-node@11.0.0",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@sindresorhus/is": "^6.1.0"
  }
}
